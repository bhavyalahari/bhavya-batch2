import java.util.*;
class ArrayalistMerge
{
	public static void main(String args[]) {
	ArrayList<Integer> a=new ArrayList<Integer>();
	a.add(10);
	a.add(20);
	a.add(30);
      ArrayList<Integer> b=new ArrayList<Integer>();
b.add(100);
b.add(200);
b.add(300);
a.addAll(b);
System.out.println(a);
	}
	}

OUTPUT:-
[10,20,30,100,200,300]

