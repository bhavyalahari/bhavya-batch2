class Solution {
    public int countPrimes(int n) {
        int count=0;
        for(int i=2;i<n;i++)
        {
            int c=0;
            for(int j=2;j<i;j++)
            {
                if((i%j){=0)
                    c++;
            }
            if(c==0)
                count++;
        }
        return count;   
    }
}
